{
  "expo": {
    "name": "App",
    "slug": "App",
    "version": "1.0.0",
    "orientation": "portrait",
    "plugins": [
      "expo-build-properties"
    ],
    "icon": "./src/presentation/public/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/presentation/public/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stima"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/presentation/public/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.stima"
    },
    "web": {
      "favicon": "./src/presentation/public/favicon.png"
    }
  }
}
