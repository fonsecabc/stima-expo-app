{
  "expo": {
    "name": "Stima",
    "slug": "stima",
    "version": "1.0.0",
    "orientation": "portrait",
    "plugins": [
      "expo-build-properties"
    ],
    "icon": "./src/app/public/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/app/public/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stima"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/app/public/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.stima"
    },
    "web": {
      "favicon": "./src/app/public/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "949b9a1e-a010-44c9-a411-f74195916d60"
      }
    },
    "owner": "fonsecabc"
  }
}
